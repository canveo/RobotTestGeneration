<?xml version="1.0"?>
<launch>

  <node pkg="flightcontroller" type="velocity_calculator.py" name="velocity_calculator_node" output="screen">
  </node>

  <node pkg="flightcontroller" type="velocity_controller.py" name="velocity_controller_node" output="screen">
    <param name="gains/p_xy" type="double" value="0.15" />
    <param name="gains/i_xy" type="double" value="0.01" />
    <param name="gains/d_xy" type="double" value="0.05" />
    <param name="gains/p_z" type="double" value="1.5" />
    <param name="gains/i_z" type="double" value="0.3" />
    <param name="gains/d_z" type="double" value="0.3" />
  </node>

</launch>